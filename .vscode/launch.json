{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Next.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/node_modules/next/dist/bin/next",
            "args": ["dev"],
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "node",
        }
    ]
}